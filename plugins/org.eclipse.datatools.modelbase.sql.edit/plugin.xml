<plugin
    name = "%pluginName"
    id = "org.eclipse.datatools.modelbase.edit"
    version = "0.7.0"
    provider-name = "%providerName"
    class = "org.eclipse.datatools.modelbase.sql.schema.provider.SqlmodelEditPlugin$Implementation">

  <requires>
    <import plugin="org.eclipse.core.runtime" />
    <import plugin="org.eclipse.datatools.modelbase.sql" export="true"/>
    <import plugin="org.eclipse.emf.edit" export="true"/>
    <import plugin="org.eclipse.emf.ecore" export="true"/>
    <import plugin="org.eclipse.emf.ecore.edit" export="true"/>
  </requires>

  <runtime>
    <library name="sqlmodel.edit.jar">
      <export name="*"/>
    </library>
  </runtime>

</plugin>

